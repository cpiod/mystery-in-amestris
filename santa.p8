pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
 p={x=9*8;y=4*8}
 p.ox=p.x
 p.oy=p.y
end

function _draw()
cls(3)
palt(0,false)
palt(14,true)
--sspr(0,96,32,32,0,20,64,64)
--sspr(32,96,32,32,64,20,64,64,true)
map()
spr(1,p.x,p.y)
end

function _update60()
 if p.x==p.ox and p.y==p.oy then
	 local x,y=p.x,p.y
	 if(btn(➡️)) x+=8
	 if(btn(⬆️)) y-=8
	 if(btn(⬅️)) x-=8
	 if(btn(⬇️)) y+=8
	 if(x!=p.x)y=p.y
	 if(not fget(mget(x/8,y/8),0)) p.ox,p.oy=x,y
 else
  if(p.x<p.ox) p.x+=1
  if(p.x>p.ox) p.x-=1
  if(p.y<p.oy) p.y+=1
  if(p.y>p.oy) p.y-=1
 end
end
